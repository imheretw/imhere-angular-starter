{
  "presets": ["es2015", "es2016", "es2017"],
  "plugins": [
    [
      "transform-runtime", {
        "polyfill": false,
        "regenerator": true,
      },
    ],
    "transform-object-rest-spread",
    "syntax-decorators",
    "dynamic-import-webpack"
  ],
  "env": {
    "test": {
      "presets": ["es2015", "es2016", "es2017"],
      "plugins": [
        "babel-plugin-webpack-alias"
      ]
    }
  }
}
